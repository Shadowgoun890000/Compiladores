// Ejemplo con estructuras de control

let x = 10;
let y = 5;

// Sentencia if-else
if (x > y) {
    let mayor = x;
} else {
    let mayor = y;
}

// Sentencia while
let contador = 0;
while (contador < 10) {
    contador = contador + 1;
}

// Sentencia for
for (let i = 0; i < 5; i = i + 1) {
    let cuadrado = i * i;
}

