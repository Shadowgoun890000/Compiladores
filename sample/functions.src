// Ejemplo con funciones

function saludar(nombre) {
    let mensaje = "Hola, " + nombre;
    return mensaje;
}

function suma(a, b) {
    return a + b;
}

function factorial(n) {
    if (n == 0) {
        return 1;
    } else {
        return n * factorial(n - 1);
    }
}

